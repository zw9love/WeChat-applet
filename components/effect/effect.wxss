/* components/effect/effect.wxss */
.container{
  display: flex;
  height:  50px;
  background-color: #fff;
  border-bottom: 1px solid #eee;
  align-items: center;
  justify-content: space-between; 
}

.left-wrapper{
  display: flex;
  /* background-color: yellow; */
  align-items: center;
}

.left-wrapper  .logo{
  width:  36px;
  height:  36px;
}

.left-wrapper .info{
  font-size: 13px;
  margin-left: 10px;
}

.right-wrapper{
  display: flex;
  /* background-color: yellow; */
  align-items: center;
  margin-right: 10px
}

.right-wrapper  .logo{
  width:  8px;
  height:  13px;
  margin-left: 10px;
}

.right-wrapper .info{
  font-size: 13px;
}

.navigator-hover{
  background-color: rgba(0, 0, 0, 0.1); 
  opacity: 0.7;
}